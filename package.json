{
  "name": "rw-components",
  "version": "0.1.0",
  "description": "A collection of components to use in WRI's projects",
  "main": "dist/index.js",
  "scripts": {
    "test": "eslint --ext .js --ext .jsx .",
    "start": "start-storybook -p 9000",
    "style": "node-sass --include-path ./node_modules/ ./src/styles/index.scss ./dist/style.css",
