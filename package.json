  "license": "MIT",
  "bugs": {
    "url": "https://github.com/resource-watch/rw-components/issues"
  },
  "homepage": "https://github.com/resource-watch/rw-components#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "bourbon": "^4.3.3",
    "brace": "^0.10.0",
    "classnames": "^2.2.5",
    "datalib": "^1.7.3",
    "jiminy": "^0.1.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-ace": "^4.1.6",
    "react-dom": "^15.4.1",
    "react-select": "^1.0.0-rc.3",
