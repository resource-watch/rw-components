    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-vizzuality": "^1.0.3",
    "gulp": "^3.9.1",
    "node-sass": "^4.0.0",
    "postcss-loader": "^1.3.0",
    "sass-loader": "^6.0.0",
